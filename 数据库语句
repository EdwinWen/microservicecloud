drop database if exists cloudDB01;  

create database cloudDB01;  

use cloudDB01;  
CREATE TABLE IF NOT EXISTS `dept`(
   `deptno` INT(11) NOT NULL AUTO_INCREMENT,
   `dname` VARCHAR(100) NOT NULL,
   `db_source` VARCHAR(40) NOT NULL,
   PRIMARY KEY ( `deptno` )
)ENGINE=InnoDB DEFAULT CHARSET=utf8;

delete from  dept;
insert into dept(dname,db_source) values('软件部','db01');
insert into dept(dname,db_source) values('人事部','db01');
insert into dept(dname,db_source) values('销售部','db01');
select * from dept;
